<Window x:Class="Stopwatch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stopwatch"
        mc:Ignorable="d" SizeToContent="WidthAndHeight"
        Title="Stopwatch" Style="{StaticResource WindowStyle}">



    <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20,10,20,10">
        <WrapPanel DockPanel.Dock="Top" HorizontalAlignment="Right">
            <Button Style="{StaticResource SettingsButton}" Name="SettingsBtn">
                <Image Source="Project files/settings icon.png" ></Image>
            </Button>
        </WrapPanel>

        <Frame Name="ContentFame" NavigationUIVisibility="Hidden"></Frame>
    </DockPanel>
</Window>


